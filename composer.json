{
    "name": "jinexus/jinexus-sage",
    "type": "wordpress-theme",
    "license": "MIT",
    "description": "JiNexus Sage is a WordPress theme based on Roots Sage Advanced WordPress starter theme with Tailwind CSS and Laravel Blade",
    "homepage": "https://roots.io/sage/",
    "authors": [
        {
            "name": "JiNexus",
            "email": "jinvirle@gmail.com",
            "homepage": "https://github.com/jinexus"
        }
    ],
    "keywords": [
        "jinexus-sage",
        "wordpress"
    ],
    "support": {
        "issues": "https://github.com/JiNexus/jinexus-sage/issues"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "laravel/pint": "^1.13"
    },
    "suggest": {
        "log1x/sage-directives": "A collection of useful Blade directives for WordPress and Sage (^1.0)."
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "acorn": {
            "providers": [
                "App\\Providers\\ThemeServiceProvider"
            ]
        }
    }
}
